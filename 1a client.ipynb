    "import socket\n",
    "s=socket.socket()\n",
    "s.bind(('localhost',8000))\n",
    "s.listen(5)\n",
    "c,addr=s.accept()\n",
    "while True:\n",
    "    i=input(\"Enter a data: \")\n",
    "    c.send(i.encode())\n",
    "    ack=c.recv(1024).decode()\n",
    "    if ack:\n",
    "        print(ack)\n",
    "        continue\n",
    "    else:\n",
    "        c.close()\n",
    "        break"
   ]
  }
